          }
        ],
        "information": [
          {
            "method": "xpath",
            "arg": "//div[@class=\"da_black\"]/p/text()"
          }
        ],
        "date": [
          {
            "method": "xpath",
            "arg": "//div[@class=\"info\"]/div/b/text()"
  
          }
        ],
  
        "editor": [
          {
            "method": "xpath",
            "arg": "//div[@class=\"info\"]//a/text()"
          }
        ]

      }
    },
        "headers": {
            "authority": "analytics.dvidshub.net",
